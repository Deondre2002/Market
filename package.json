{
  "name": "market",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "vitest",
    "db:schema": "psql -d market -f db/schema.sql",
    "db:seed": "node --env-file=.env db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "supertest": "^7.1.0",
    "vitest": "^3.1.2"
  },
  "version": "1.0.0",
  "description": "Market is a versatile and robust e-commerce platform that allows users to browse and purchase a wide selection of products.",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deondre2002/Market.git"
  },
